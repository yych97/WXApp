<canvas canvas-id="myCanvas"
  style="width:100%; height:400px;"
  height="500px"
  bindtouchstart="start"
  bindtouchmove="move"
  bindtouchend="end"/>

<view hidden="{{hidden}}">
  Coordinates: ({{x}}, {{y}})
</view>
<button bindtap='loadImg'>加载谷歌地图</button>
<view style='width:100%; height:10%'>
  <button bindtap='zoomIn' style='width:50%; height:10%'>放大</button>
  <button bindtap='zoomOut' style='width:50%; height:10%'>缩小</button>
</view>
